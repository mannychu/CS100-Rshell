echo
echo Testing Append Redirection...
echo
echo Appending test cases 1, 2, 3 through append command
echo psuedo1 >> ex1
echo pseudo2 >> ex1
echo pseudo3 >> ex1
echo 
echo sugoi > ex2
chmod 711 ex1
chmod 711 ex2
cat < ex1 >> ex2
echo Removing ex1 and ex2 files
rm ex1
rm ex2
echo
echo Program will exit now.
exit
